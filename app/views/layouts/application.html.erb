<!DOCTYPE html>
<html>
  <head>
    <title>MjdmRocks</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="top-bar">
      <img src="/logo2.png" height="70">
      <h1 id="company-name">Rutherford Rocket Labs</h1>
      <%= form_tag sessions_path, method: :delete do %>
        <%= submit_tag 'Logout', id: 'logout' %>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
