<h1>Inventory</h1>
<table>
  <tr>
    <th>Part Name</th>
    <th>Part Number</th>
    <th>Quantity</th>
    <th>Max Quantity</th>
  </tr>
  <% @parts.each do |part| %>
    <tr>
      <td><%= part.name %></td>
      <td><%= part.part_number %></td>
      <td><%= Part.overall_total(part) %></td>
      <td><%= part.max_quantity %></td>
    </tr>
  <% end %>
</table>
